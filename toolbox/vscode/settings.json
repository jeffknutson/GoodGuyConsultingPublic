// Place your settings in this file to overwrite the default settings
{
    "editor.autoClosingBrackets": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    // "editor.useTabStops": false,
    "editor.tabSize": 2,
    //from http://stackoverflow.com/questions/32911977/prevent-autocomplete-in-visual-studio-code
    // Place your settings in this file to overwrite default and user settings. {
    // Controls if quick suggestions should show up while typing
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    // Controls the delay in ms after which quick suggestions will show up.
    "editor.quickSuggestionsDelay": 10,
    // Enable word based suggestions
    "editor.wordBasedSuggestions": false,
    "window.zoomLevel": 0,
    "search.exclude": {
        "**/dist": true,
        "**/build": true
    },
    "files.autoSave": "off",
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "editor.format.wrapAttributes": "off",
    "explorer.openEditors.visible": 0,
    "jsDocCompletion.enabled": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "typescript.check.npmIsInstalled": false,
    "editor.wrappingIndent": "none",
    "editor.wordWrapColumn": 9999999,
    "workbench.sideBar.location": "left",
    "html.format.wrapLineLength": 0,
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.quickSuggestions": false
    },
    "terminal.external.osxExec": "zsh",
    "explorer.confirmDelete": false,
    "html.autoClosingTags": false,
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "[javascript]": {
        "editor.tabSize": 2
    },
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "welcomePage",
    "breadcrumbs.enabled": false
}